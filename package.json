{
  "name": "web-fullstack-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "yarn dev:docker",
    "dev": "yarn dev:next",
    "dev:next": "next dev",
    "dev:docker": "zx ./docker.mjs",
    "dev:migrate": "prisma migrate dev --name",
    "dev:generate": "prisma generate",
    "dev:seed": "yarn prisma db seed",
    "build:deploy": "prisma migrate deploy",
    "prebuild": "concurrently -c green.bold,blue.bold 'yarn:dev:generate' 'yarn:build:deploy'",
    "build": "next build",
    "postinstall": "concurrently -c green.bold,blue.bold 'yarn:dev:docker' 'yarn:dev:generate' && yarn build:deploy",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "license": "MIT",
  "keywords": [
    "web",
    "fullstack",
    "starter"
  ],
  "author": "Özgür Isbert",
  "repository": {
    "url": "https://github.com/itsstacked/stacked-chakra-ui.git"
  },
  "dependencies": {
    "@chakra-ui/react": "2.1.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fontsource/inter": "4.5.10",
    "@next-auth/prisma-adapter": "1.0.3",
    "@prisma/client": "3.14.0",
    "@trpc/client": "9.23.6",
    "@trpc/next": "9.23.6",
    "@trpc/react": "9.23.6",
    "@trpc/server": "9.23.6",
    "framer-motion": "6.3.3",
    "next": "12.1.6",
    "next-auth": "4.3.4",
    "next-seo": "5.4.0",
    "nprogress": "0.2.0",
    "phosphor-react": "1.4.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-query": "3.39.0",
    "superjson": "1.9.1",
    "zod": "3.17.2"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "concurrently": "7.2.1",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "prisma": "3.14.0",
    "ts-node": "10.8.0",
    "typescript": "4.6.4",
    "zx": "6.1.0"
  }
}
