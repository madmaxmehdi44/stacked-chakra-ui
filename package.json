{
  "name": "web-fullstack-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -c green.bold,blue.bold 'yarn:dev:docker' 'yarn:dev:next'",
    "dev:next": "next dev",
    "predev:docker": "zx ./checkfiles.mjs",
    "dev:docker": "docker compose up -d",
    "dev:migrate": "prisma migrate dev --name",
    "dev:generate": "prisma generate",
    "dev:seed": "yarn prisma db seed",
    "build:deploy": "prisma migrate deploy",
    "prebuild": "concurrently -c green.bold,blue.bold 'yarn:dev:generate' 'yarn:build:deploy'",
    "build": "next build",
    "postinstall": "concurrently -c green.bold,blue.bold 'yarn:dev:docker' 'yarn:dev:generate' && yarn build:deploy",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "license": "MIT",
  "keywords": [
    "web",
    "fullstack",
    "starter"
  ],
  "author": "Özgür Isbert",
  "repository": {
    "url": "https://github.com/oezguerisbert/web-fullstack-starter.git"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/inter": "^4.5.7",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.12.0",
    "@trpc/next": "^9.22.0",
    "@trpc/react": "^9.22.0",
    "@trpc/server": "^9.22.0",
    "framer-motion": "^6",
    "next": "12.1.4",
    "next-auth": "^4.3.3",
    "next-seo": "^5.4.0",
    "phosphor-react": "^1.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "superjson": "^1.8.2",
    "typescript": "^4.6.3",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "concurrently": "^7.1.0",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "zx": "^6.1.0"
  }
}
